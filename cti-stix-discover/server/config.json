{
  "restApiRoot": "/cti-stix-discover-api",
  "host": "0.0.0.0",
  "port": 3000,
  "remoting": {
    "context": false,
    "rest": {
      "normalizeHttpPath": true,
      "xml": false,
      "supportedTypes": [
        "application/vnd.api+json",
        "application/json",
        "json"
      ]
    },
    "json": {
      "strict": false,
      "limit": "100kb"
    },
    "urlencoded": {
      "extended": true,
      "limit": "100kb"
    },
    "cors": false,
    "handleErrors": false
  },
  "legacyExplorer": false
}
